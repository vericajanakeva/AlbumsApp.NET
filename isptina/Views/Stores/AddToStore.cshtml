@model isptina.Models.AddToStoreModel
@{
    ViewBag.Title = "AddToStore";
}

<h2>Add To Store</h2>

@using (Html.BeginForm("AddToStore","Stores"))
{

    <div class="form-group">
    @Html.DropDownListFor(m => m.selectedAlbum,
          new SelectList(Model.albums, "Id", "Name"),
          new { @class = "form-control"}
     )
      @Html.HiddenFor(m => m.storeId)
    </div>
    <button type="submit" class="btn btn-primary">Save</button>


}

